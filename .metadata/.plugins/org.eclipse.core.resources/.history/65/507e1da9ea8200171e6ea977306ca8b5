package œﬂ≥Ã≤‚ ‘;

public class LockTest {


	int i = 10;
	public static void main(String[] args) {
	
		
		
	}
	
	public class Thread1 implements Runnable{

		
		@Override
		public void run() {

			while(true){
				
				synchronized(this){
					
					i = (i--) / 0;
					System.out.println(i);
				}
			}
		}
		
	}
	
	public class Thread2 implements Runnable{

		
		@Override
		public void run() {

			while(true){
				
				synchronized(this){
					
					i = (i--) / 0;
					System.out.println(i);
				}
			}
		}
		
	}
	
}
