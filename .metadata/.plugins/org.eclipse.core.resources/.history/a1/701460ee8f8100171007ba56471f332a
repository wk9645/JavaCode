package com.wk.test;

import javax.management.timer.TimerMBean;

public class ThreadTest {

	

}


class Climb{
	
	
	int time;
	int step;

	public Climb(){
		
		time = 0;
		step = 0;
	}

}

class Young implements Runnable{

	Climb climb;
	public Young(Climb climb) {
		this.climb = climb;
	}
	
	@Override
	public void run() {
		
		step+=100;
	}
	
	
}
